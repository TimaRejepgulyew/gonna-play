{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": ["react-native"],
    "paths": {
      "@/*": ["./app/*"],
      "api/*": ["./app/api/*"],
      "assets/*": ["./app/assets/*"],
      "components/*": ["./app/components/*"],
      "screens/*": ["./app/screens/*"],
      "state/*": ["./app/state/*"],
      "theme/*": ["./app/theme/*"],
      "~/*": ["./~/*"]
    }
  }
}
